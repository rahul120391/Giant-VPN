<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@drawable/back1">


    <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:id="@+id/relativeLayout"
            android:background="@drawable/crlcrnr">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/VPNname"
                android:layout_centerInParent="true"
                android:id="@+id/textView10"
                android:textSize="20dp"
                android:textStyle="bold"
               />

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/set"
                android:id="@+id/sett"
                android:onClick="v"
                android:textColor="@color/blue"
                android:textSize="16dp"
                android:layout_alignTop="@+id/webv"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/webv"
                android:textColor="@color/blue"
                android:textSize="16dp"
                android:id="@+id/webv"
               android:layout_marginLeft="5dp"
                android:layout_centerVertical="true"/>
    </RelativeLayout>


        <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:id="@+id/imageView4" android:layout_below="@+id/relativeLayout"
            android:layout_centerHorizontal="true"
            android:background="@drawable/shield"/>

    <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView4"
            android:id="@+id/relativeLayout2"
            android:background="@drawable/crlcrnr"
            android:layout_marginTop="4dp"
            >
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"
                android:text="@string/disconnect"
                android:id="@+id/textView9"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="8dp"
                android:layout_marginTop="4dp"
                android:textColor="@color/red"/>
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="35dp"
                android:paddingRight="35dp"
                android:text="@string/connect"
                android:paddingBottom="6dp"
                android:paddingTop="6dp"
                android:textSize="22dp"
                android:id="@+id/conn"
                android:background="@drawable/circularcorner"
                android:layout_below="@+id/textView9" android:layout_centerHorizontal="true"/>
    </RelativeLayout>


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:background="@drawable/crlcrnr"
            android:id="@+id/relativeLayout5"
            android:layout_marginTop="13dp"
            android:layout_below="@+id/relativeLayout2" android:layout_alignParentLeft="true">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/location"
                android:id="@+id/textView8"
                android:textSize="18dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="8dp"/>
        <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:id="@+id/imageView5"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:background="@drawable/uk"
                android:layout_marginLeft="8dp"/>
        <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:id="@+id/imageView6"
                android:background="@drawable/de"
                android:layout_alignTop="@+id/imageView5"
                android:layout_toRightOf="@+id/imageView5"
                android:layout_marginLeft="12dp"/>

        <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:id="@+id/imageView7"
                android:background="@drawable/us"
                android:layout_alignTop="@+id/imageView6"
                android:layout_toRightOf="@+id/imageView6"
                android:layout_marginLeft="12dp"/>

        <ImageView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:id="@+id/imageView8"
                android:background="@drawable/nl"
                android:layout_alignTop="@+id/imageView7"
                android:layout_toRightOf="@+id/imageView7"
                android:layout_marginLeft="12dp"/>

    </RelativeLayout>


    <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:id="@+id/relativeLayout3"
            android:layout_marginTop="8dp"
            android:background="@drawable/crlcrnr"
            android:layout_below="@+id/relativeLayout5"
            android:layout_alignParentLeft="true">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/adblocker"
                android:textSize="18dp"
                android:id="@+id/textView7"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                />

        <ToggleButton
            android:layout_width="45dp"
            android:layout_height="30dp"
            android:background="@drawable/check"
            android:textOff=""
            android:textOn=""
            android:id="@+id/switch1"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

    <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="55dp"
            android:id="@+id/relativeLayout4"
            android:layout_marginTop="8dp"
            android:background="@drawable/crlcrnr" android:layout_below="@+id/relativeLayout3"
            android:layout_alignParentLeft="true">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/alwayscnct"
                android:textSize="18dp"
                android:id="@+id/textView6"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"/>

        <ToggleButton
            android:layout_width="45dp"
            android:layout_height="30dp"
            android:background="@drawable/check"
            android:textOff=""
            android:textOn=""
            android:id="@+id/alwaysConnected"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>


        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="85dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/crlcrnr"
            android:layout_below="@+id/relativeLayout4"
            android:layout_alignParentLeft="true">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/time"
                android:textSize="18dp"
                android:id="@+id/textView" android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="9dp"
                android:layout_marginLeft="10dp"
                />
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/gun"
                android:textSize="18dp"
                android:id="@+id/textView3"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_marginBottom="9dp"
                android:layout_marginLeft="10dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/acc"
                android:textSize="18dp"
                android:id="@+id/textView4" android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="9dp"
                android:layout_marginRight="10dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/pacc"
                android:textSize="18dp"
                android:id="@+id/textView5"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_marginBottom="4dp"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="10dp"/>
    </RelativeLayout>

</RelativeLayout>